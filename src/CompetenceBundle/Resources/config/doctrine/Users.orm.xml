<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CompetenceBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="role" type="string" column="role" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rememberToken" type="string" column="remember_token" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="nom" type="string" column="nom" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cp" type="string" column="cp" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="string" column="telephone" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dob" type="date" column="dob" nullable="false"/>
  </entity>
</doctrine-mapping>
