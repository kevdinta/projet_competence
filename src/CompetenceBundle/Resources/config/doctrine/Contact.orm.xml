<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CompetenceBundle\Entity\Contact" table="contact">
    <id name="idContact" type="integer" column="id_contact">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomContact" type="string" column="nom_contact" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomContact" type="string" column="prenom_contact" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adreseContact" type="string" column="adrese_contact" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cpContact" type="string" column="CP_contact" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="villeContact" type="string" column="ville_contact" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telfixeContact" type="string" column="telFixe_contact" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telmobileContact" type="string" column="telMobile_contact" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
